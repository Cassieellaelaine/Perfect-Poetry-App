<!-- Loop over each object in the projects array -->

<div class="row mb-4 poetry">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/poetry/{{id}}">{{name}}</a>
    </h2>
    <p>Created by {{name}} on {{format_date date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{description}}
    </p>
  </div>
</div>

